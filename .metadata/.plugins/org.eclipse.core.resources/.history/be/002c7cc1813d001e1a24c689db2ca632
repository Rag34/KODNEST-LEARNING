package conditionalconstructs;

public class labelledforloop {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i=1,j=1;
		rajini:while(i<=5) {
			while(j<=5) {
				System.out.println("*");
				i++;
				j++;
				break rajini;
			}
			System.out.println();
		}

	}

}
